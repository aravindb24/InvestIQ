{
  "manifest_version": 3,
  "name": "Stock Monitor",
  "version": "1.0",
  "description": "A simple Chrome extension to look up stock prices.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./icons/icon.png",
      "48": "./icons/icon.png",
      "128": "./icons/icon.png"
    }
  }
}
